@model Produkt

<div id="@("produkt" + Model.Id)>
    <div class="row">
        <div>@Model.Name</div>
        <div>@Model.Price</div>
        <div>
            <button class="btn btn-warning" onclick="LinkUpdate('@Url.Action("Edit", "Produkt", new {id = Model.Id })' , '@("produkt" + Model.Id)')">
                Edit
            </button>
        </div>
    </div>

</div>